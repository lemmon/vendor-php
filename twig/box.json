{
  "finder": [
    {
      "name": "*",
      "notName": [
        "composer.json",
        "CHANGELOG",
        "phpunit.xml.dist",
        "*.rst"
      ],
      "exclude": [
        "Tests",
        "doc"
      ],
      "in": "vendor"
    }
  ],
  "main": "vendor/autoload.php",
  "output": "../twig.phar",
  "stub": true,
  "shebang": false
}
